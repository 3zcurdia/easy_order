<% content_for(:keywords, @merchant.keyword_list.to_s) %>
<% content_for(:title, @merchant.name) %>

<% cache @merchant do %>
  <%= render 'pages/header', merchant: @merchant %>
  <% cache @merchant.menu do %>
    <%= render 'pages/menu', merchant: @merchant, menu_items: @menu_items %>
  <% end %>
<% end %>

<%= render 'footer' %>
